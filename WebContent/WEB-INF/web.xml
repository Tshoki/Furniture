<?xml version="1.0" encoding="UTF-8"?>
<web_1:web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee " xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd " xmlns:web_1="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <web_1:display-name>furniture</web_1:display-name>
  <web_1:welcome-file-list>
    <web_1:welcome-file>index.jsp</web_1:welcome-file>
  </web_1:welcome-file-list>
  <web_1:filter>
    <web_1:filter-name>struts2</web_1:filter-name>
    <web_1:filter-class>org.apache.struts2.dispatcher.ng.filter.StrutsPrepareAndExecuteFilter</web_1:filter-class>
  </web_1:filter>
  <web_1:filter-mapping>
    <web_1:filter-name>struts2</web_1:filter-name>
    <web_1:url-pattern>/*</web_1:url-pattern>
  </web_1:filter-mapping>
  <web_1:error-page>
    <web_1:error-code>404</web_1:error-code>
    <web_1:location>/systemError.html</web_1:location>
  </web_1:error-page>
  <web_1:error-page>
    <web_1:error-code>500</web_1:error-code>
    <web_1:location>/systemError.html</web_1:location>
  </web_1:error-page>
</web_1:web-app>